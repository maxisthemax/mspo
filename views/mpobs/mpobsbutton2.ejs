<style>
    .btnpadding {
        margin: 10px;
    }
</style>
<div id="cust_buttons" class="p-5">
    <p class="h4 mb-4">Actions</p>
    <div class="row">
        <button type="button" class="btn btn-info btnpadding"
            onclick="var retVal = confirm('Do you want to restore this MPOB ?');if (retVal == true) {location.href = '/mpobs/restore/<%= mpob_s_disabled[4].data%>'};">Restore</button>
         <button type="button" class="btn btn-danger btnpadding"
            onclick="var retVal = confirm('Do you want to delete this MPOB ? (This will permanently delete the MPOB)');if (retVal == true) {location.href = '/mpobs/delete/<%= mpob_s_disabled[4].data%>'};">Delete</button>
    </div>
</div>