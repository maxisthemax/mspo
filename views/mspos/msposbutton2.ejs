<style>
    .btnpadding {
        margin: 10px;
    }
</style>
<div id="cust_buttons" class="p-5">
    <p class="h4 mb-4">Actions</p>
    <div class="row">
        <button type="button" class="btn btn-info btnpadding"
            onclick="var retVal = confirm('Do you want to restore this MSPO ?');if (retVal == true) {location.href = '/mspos/restore/<%= mspo_s_disabled[3].data%>'};">Restore</button>
         <button type="button" class="btn btn-danger btnpadding"
            onclick="var retVal = confirm('Do you want to delete this MSPO ? (This will permanently delete the MSPO)');if (retVal == true) {location.href = '/mspos/delete/<%= mspo_s_disabled[3].data%>'};">Delete</button>
    </div>
</div>