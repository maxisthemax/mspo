<style>
    .btnpadding {
        margin: 10px;
    }
</style>
<div id="ticket_buttons" class="p-5">
    <p class="h4 mb-4">Actions</p>
    <div class="row">
        <button type="button" class="btn btn-info btnpadding"
            onclick="var retVal = confirm('Do you want to restore this ticket ?');if (retVal == true) {location.href = '/tickets/restore/<%= ticket_s_disabled[13].data%>'};">Restore</button>
         <button type="button" class="btn btn-danger btnpadding"
            onclick="var retVal = confirm('Do you want to delete this ticket ? (This will permanently delete the ticket)');if (retVal == true) {location.href = '/tickets/delete/<%= ticket_s_disabled[13].data%>'};">Delete</button>
    </div>
</div>